{
    "scripts": {
        "test": "jest --coverage"
    },
    "jest": {
        "testEnvironment": "jsdom",
        "collectCoverage": true,
        "coverageDirectory": "coverage",
        "coverageReporters": [
            "text"
        ],
        "coverageThreshold": {
            "global": {
                "branches": 80,
                "functions": 80,
                "lines": 80,
                "statements": 80
            }
        }
    },
    "devDependencies": {
        "@testing-library/dom": "^10.4.0",
        "@testing-library/jest-dom": "^6.6.3",
        "jest": "^29.7.0",
        "jest-environment-jsdom": "^29.7.0"
    },
    "dependencies": {
        "jest-fixed-jsdom": "^0.0.9",
        "jspdf": "^3.0.1",
        "jspdf-autotable": "^5.0.2"
    }
}
